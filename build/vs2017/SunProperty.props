<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <SunProjectDir>$(SolutionDir)..\..</SunProjectDir>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SunProjectDir)\out\$(Configuration)_$(Platform)\</OutDir>
    <IntDir>$(SunProjectDir)\out\$(Configuration)_$(Platform)\$(ProjectName)\</IntDir>
    <IncludePath>$(SunProjectDir)\src\;$(IncludePath)</IncludePath>
    <LibraryPath>$(SunProjectDir)\src\EGE\;$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="SunProjectDir">
      <Value>$(SunProjectDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>